{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --base=/",
    "build": "tsc && vite build --base=/",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:watch:all": "vitest src",
    "test:watch:all:coverage": "test:watch:all --collectCoverage",
    "test:coverage": "cross-env TEST_PATH=/ vitest --watchAll --collectCoverageFrom=$TEST_PATH/**/** --coverage $TEST_PATH",
    "test:all:coverage": "vitest --collectCoverage",
    "test:staged": "cross-env CI=true vitest --env=jsdom --findRelatedTests",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "knip": "knip"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.25.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/material": "^7.0.1",
    "@mui/system": "^5.14.7",
    "@mui/x-date-pickers": "^7.28.2",
    "@playwright/test": "^1.52.0",
    "@react-pdf/renderer": "^4.3.0",
    "@tanstack/react-query": "^5.73.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "eslint-plugin-react": "^7.31.10",
    "jotai": "^2.12.4",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.53.0",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "6.30.1",
    "storybook": "^8.6.14",
    "typescript": "^5.8.3",
    "vite": "5.4.19",
    "vite-plugin-checker": "^0.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^22.15.29",
    "@types/prettier": "^2.7.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitest/coverage-v8": "2.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "knip": "^5.59.1",
    "prettier": "^3.0.0",
    "vitest": "^3.1.3"
  },
  "husky": {},
  "overrides": {
    "@storybook/core-server": {
      "ws": "8.18.0"
    },
    "jsdom": {
      "ws": "8.18.0"
    },
    "puppeteer-core": {
      "ws": "6.2.3"
    }
  },
  "resolutions": {
    "esbuild": "^0.25.0"
  }
}
